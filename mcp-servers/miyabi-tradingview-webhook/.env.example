# Miyabi TradingView Webhook Server
# Copy this file to .env and fill in your values

# Server Port
PORT=3456

# Slack Webhook URL (optional)
# Get from: https://api.slack.com/messaging/webhooks
SLACK_WEBHOOK_URL=

# Discord Webhook URL (optional)
# Get from: Server Settings > Integrations > Webhooks
DISCORD_WEBHOOK_URL=

# LINE Notify URL (optional)
# Get from: https://notify-bot.line.me/
LINE_NOTIFY_URL=

# Custom Webhook URL (optional)
# Any HTTP endpoint that accepts POST requests
CUSTOM_WEBHOOK_URL=
