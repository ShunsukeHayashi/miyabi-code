# Server
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/aipartner

# Redis
REDIS_URL=redis://localhost:6379

# BytePlus API
BYTEPLUS_API_KEY=your_byteplus_api_key
BYTEPLUS_API_ENDPOINT=https://api.byteplus.com
BYTEPLUS_T2I_MODEL=your_t2i_model_id
BYTEPLUS_I2I_MODEL=your_i2i_model_id
BYTEPLUS_T2V_MODEL=your_t2v_model_id
BYTEPLUS_I2V_MODEL=your_i2v_model_id

# Gemini API (TTS)
GEMINI_API_KEY=your_gemini_api_key
GEMINI_TTS_ENDPOINT=https://generativelanguage.googleapis.com/v1

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key

# JWT
JWT_SECRET=your_jwt_secret_key_change_this_in_production
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
