{
  "name": "miyabi-commercial-agents",
  "version": "1.0.0",
  "description": "Miyabi Commercial Business Agents - All-in-One MCP Package (6 Premium Agents)",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "miyabi-commercial": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:binary": "npm run build:all",
    "build:all": "npm run build:macos && npm run build:windows && npm run build:linux",
    "build:macos": "pkg dist/index.js --target node20-macos-x64 --output bin/miyabi-commercial-macos",
    "build:windows": "pkg dist/index.js --target node20-win-x64 --output bin/miyabi-commercial-win.exe",
    "build:linux": "pkg dist/index.js --target node20-linux-x64 --output bin/miyabi-commercial-linux",
    "pack": "npx @anthropic-ai/mcpb pack"
  },
  "keywords": [
    "mcp",
    "miyabi",
    "business-agents",
    "commercial",
    "sns-strategy",
    "content-creation",
    "youtube",
    "marketing",
    "analytics",
    "crm"
  ],
  "author": "Miyabi Technologies",
  "license": "Proprietary",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "zod": "^3.23.8",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [],
    "targets": [
      "node20-macos-x64",
      "node20-win-x64",
      "node20-linux-x64"
    ],
    "outputPath": "bin"
  }
}
