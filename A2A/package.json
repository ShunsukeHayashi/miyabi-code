  "repository": {
    "type": "git",
    "url": "https://github.com/miyabi-dev/a2a.git"
  },
  "devDependencies": {
    "@google/genai": "^1.31.0",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
