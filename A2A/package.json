{
  "name": "@miyabi/a2a",
  "version": "1.0.0",
  "description": "A2A (Agent-to-Agent) Multi-Agent Communication Protocol for Claude Code",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "a2a": "./bin/a2a.js",
    "a2a-bootstrap": "./scripts/a2a_bootstrap.sh",
    "a2a-health": "./scripts/a2a_health.sh",
    "a2a-scale": "./scripts/a2a_scale.sh"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "bootstrap": "./scripts/a2a_bootstrap.sh",
    "health": "./scripts/a2a_health.sh",
    "scale": "./scripts/a2a_scale.sh",
    "dashboard": "tsx src/dashboard.ts",
    "state": "tsx src/state.ts",
